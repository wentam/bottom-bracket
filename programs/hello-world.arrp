(asm
  (section .text
    (global _start))

  (section .rodata
    (label msg) (db "Greetings spheroid." 10)
    (label msg_len) (equ 20))

  (section .text
    (label _start
      (mov rdi 1)
      (mov rsi msg)
      (mov rdx msg_len)
      (mov rax 1)
      (syscall)

      (mov rdi 0)
      (mov rax 60)
      (syscall))))
